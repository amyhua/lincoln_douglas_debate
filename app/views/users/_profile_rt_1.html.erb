        <span class="user-social"><%= image_tag 'twitter_icon_black.png', class: 'icon' %>Twitter</span>
      </a><br><br>
    <% end %>
    <% unless @user.social_email.nil? %>
      <a href="mailto:<%= @user.social_email%>">
