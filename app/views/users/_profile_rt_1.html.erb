      <a href="<%= @user.googleplus%>">
        <span class="user-social"><%= image_tag 'googleplus_icon_black.png', class: 'icon' %>Google+</span>
      </a><br>
    <% end %>
    <% unless @user.facebook.nil? %>
      <a href="<%= @user.facebook%>">
        <span class="user-social"><%= image_tag 'facebook_icon_black.png', class: 'icon' %>Facebook</span>
        </a><br>
    <% end %>
    <% unless @user.twitter.nil? %>
