		<fieldset>

	  <div class="panel panel-default">

	    <div class="panel-heading">
	      <h4 class="panel-title">
	        <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapse-<%=f.object.id%>">
	          <%= f.object.name || 'New Division' %>
	        </a>
	      </h4>
	    </div>
	    <div id="collapse-<%=f.object.id%>" class="panel-collapse collapse">
	      <div class="panel-body">
	      	<div class="row">
					  <div class="col-md-7 division-name">
			      	<%= f.input :name, label: 'Rename:', placeholder: 'Division Name', error: false %></h4><br>
							<%= f.hidden_field :_destroy %>
					  </div>
						<div class="col-md-5 cancellation"><%= link_to 'Delete entire division, registrations, and rounds.', '#', class: 'remove_fields cancellation-link' %><br>
						</div>
	      	</div>



   <% num_brackets = set_num_brackets(f) %>


    	<span class="info">Click to edit.</span>
					<table class="table table-hover table-responsive">
						<tr>
							<th>#</th>
							<th>Round</th>
			<% if num_brackets <= 1 %>
							<th>Start Time</th>
			<% else %>
				<% for i in (1..num_brackets) %>
							<th><%=i.ordinalize%> Bracket</th>
				<% end %>
			<% end %>
					</tr>
					<fieldset>

						<% f.object.rounds.each.with_index do |round, index| %>

							<%= f.simple_fields_for :rounds, round do |fr| %>
								<%= render 'round_fields', f: fr, num_brackets: num_brackets, index: index %>
							<% end %>

						<% end %>
					</fieldset>
					<tr class="row-count">
						<td></td>
						<td><%= link_to_add_fields '+New Round', f, :rounds %><td>
						<% (num_brackets -1).times do %>
							<td></td>
						<% end %>
					</tr>
					</table>


<br>
					<a class="btn" data-toggle="collapse" data-target="#collapse-<%= f.object.id %>">
			  		Collapse
					</a>

	      </div>
	    </div>
	  </div>



		</fieldset>
