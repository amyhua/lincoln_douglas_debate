<%= form_for [@tournament],
			:html => { :multipart => true } do |f| %>
	<%= f.error_messages %>
	<p>
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</p>
	<p>
		<%= f.label :description %><br>
		<%= f.text_area :description %>
	</p>
	<p> <%= f.label :asset, "File" %><br>
		<%= f.file_field :asset %>
</p>
<br>
	<%= f.submit %>

<% end %>
<br><br>

